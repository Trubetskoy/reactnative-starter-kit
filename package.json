{
  "name": "reactNativeStarterKit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "upgrade": "node node_modules/react-native/local-cli/cli.js upgrade",
    "test": "jest",
    "standard-test": "standard --verbose",
    "standard-fix": "standard --fix",
    "clear-cache": "watchman watch-del-all && rm -rf node_modules/ && yarn cache clean && yarn install",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "gradle-clean": "cd android && ./gradlew clean",
    "build-release-android": "cd android && ./gradlew assembleRelease"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "immutability-helper": "^3.0.0",
    "react": "16.6.3",
    "react-native": "0.58.1",
    "react-native-firebase": "^5.2.1",
    "react-native-gesture-handler": "^1.0.15",
    "react-native-offline": "^4.1.2",
    "react-navigation": "^3.0.9",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "standard": "^12.0.1"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.0.0",
    "jest": "24.0.0",
    "metro-react-native-babel-preset": "0.51.1",
    "react-test-renderer": "16.6.3"
  },
  "jest": {
    "preset": "react-native"
  },
  "standard": {
    "ignore": [
      "__tests__"
    ],
    "parser": "babel-eslint"
  }
}
