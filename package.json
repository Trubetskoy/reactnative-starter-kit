{
  "name": "reactNativeStarterKit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "eject": "node node_modules/react-native/local-cli/cli.js eject",
    "test": "jest",
    "standard-test": "standard --verbose",
    "standard-fix": "standard --fix",
    "clear-cache": "watchman watch-del-all && rm -rf node_modules/ && yarn cache clean && yarn install",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "gradle-clean": "cd android && ./gradlew clean",
    "build-release-android": "cd android && ./gradlew assembleRelease"
  },
  "dependencies": {
    "add": "^2.0.6",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "immutability-helper": "^3.0.0",
    "react": "16.8.3",
    "react-native": "0.59.5",
    "react-native-firebase": "^5.3.1",
    "react-native-gesture-handler": "^1.1.0",
    "react-navigation": "^3.8.1",
    "react-redux": "^7.0.2",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "standard": "^12.0.1",
    "yarn": "^1.15.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "babel-jest": "^24.7.1",
    "jest": "^24.7.1",
    "metro-react-native-babel-preset": "^0.53.1",
    "react-test-renderer": "16.8.3"
  },
  "jest": {
    "preset": "react-native"
  },
  "standard": {
    "ignore": [
      "__tests__"
    ],
    "parser": "babel-eslint"
  }
}
